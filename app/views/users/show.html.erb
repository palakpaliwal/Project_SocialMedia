<div class="row justify-content-md-center profile-wrapper">
 <div class="col-md-4 text-center">
  <%= image_tag @user.profile_image, width: '152', height: '152', class: "round-img"%>
 </div>
 <div class="col-md-8">
  <div class="row">
   <p class="user_name"><%= @user.user_name %></p>
  </div>
  <div class="row">
  <p class="email"><%= @user.email %></p>
  </div>
 </div>
</div>